<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Customer>Service history</title>
    <!-- bootstrap Links -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto&display=swap" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/lib/bootstrap3/bootstrapcss/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/lib/bootstrap3/bootstrapcss/bootstrap-theme.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/css/intlTelInput.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.1/jquery.validate.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.min.js"></script>

    <link href="~/css/site.css" rel="stylesheet" />

</head>
<body>
    <!-- Header -->
    <header>
        <nav class="navbar navbar-inverse Navbarcolor">
            <div class="container-fluid Navbarcolor">
                <div class="navbar-header Navbarcolor">
                    <a class="navbar-brand Navbarcolor" asp-controller="Home" asp-action="Index">
                        <img src="~/img/white-logo.png" class="nav-other-logo img-responsive" alt="logo">
                    </a>
                </div>

                <ul class="nav navbar-nav navbar-right">
                    <li class="Navbarcolor">
                        <a href="#" class="Rounded-Rectangle-other">Book now</a>
                    </li>
                    <li><a href="#">Prices & Services</a></li>
                    <li><a href="#">Warranty</a></li>
                    <li><a href="#">Blog</a></li>
                    <li><a href="#">Contact</a></li>
                    <li><a href="#" class="scroll-link"><img src="~/img/admin-user.png" class="nav-noti-icon img-responsive"></a></li>
                    <li><a href="#" class="scroll-link"><img src="~/img/icon-notification.png" class="nav-noti-icon img-responsive"></a></li>
                    <li class="dropdown Navbarcolor">
                        <a class="dropdown-toggle  Navbarcolor" data-toggle="dropdown" href="#">
                            <img src="~/img/go-up.png" class="nav-noti-icon img-responsive" />
                            <ul class="dropdown-menu Navbarcolor">
                                <li><a asp-controller="Customer" asp-action="CustomerDashboard">Dashboard</a></li>
                                <li><a asp-controller="Customer" asp-action="Mysettings">My Settings</a></li>
                                <li><a asp-controller="Customer" asp-action="Logout">Logout</a></li>
                            </ul>
                    </li>
                </ul>
            </div>
        </nav>

    </header>
    <!-- Header End -->
    <div>
        @RenderBody()
    </div>
    <script src="~/js/site.js" asp-append-version="true"></script>
</body>

</html>
